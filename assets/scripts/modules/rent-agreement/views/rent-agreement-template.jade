                  input.form-control(type='text' name='license_expiration_date')

            .col-md-12
              .col-md-12
                hr
          .row
            .form-group.col-md-6.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | Address 1
                span.required(aria-required='true') *
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='Address 1', data-container='body')
                  input.form-control(type='text' name='address_1')
            .form-group.col-md-6.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | Address 2
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='Address 2', data-container='body')
                  input.form-control(type='text' name='address_2')
          .row
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | City
                span.required(aria-required='true') *
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='City', data-container='body')
                  input.form-control(type='text' name='city')
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.control-label.col-md-12
                | Country
                span.required(aria-required='true') *
              .col-md-12
                select.form-control(name='country')
                  option(value='') Select...
                  option(value='USA') USA
                  option(value='Canada') Canada
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.control-label.col-md-12
                | State
                span.required(aria-required='true') *
              .col-md-12
                select.form-control(name='state')
                  option(value='') Select...
