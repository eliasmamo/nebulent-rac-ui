.row
  .col-md-12.col-sm-12.col-xs-12.rent-agreement-portlet
    .portlet.light
      .portlet-title
          .col-md-4.caption
            i.fa.fa-user.font-purple-plum
            span.caption-subject.bold.font-purple-plum.uppercase
              | Customer
            span.caption-helper (first name, last name, id)
          .col-md-4
            .form-group
              .radio-list
                label.radio-inline
                  #uniform-optionsRadios4.radio
                    span.checked
                      input#optionsRadios4(type='radio', name='customerChoiceRadios', value='new')
                  |  New Customer
                label.radio-inline
                  #uniform-optionsRadios5.radio
                    span
                      input#optionsRadios5(type='radio', name='customerChoiceRadios', value='existing', checked='')
                  |  Existing Customer
          .inputs.col-md-3.left
            .portlet-input.input-inline.input-small
              select.form-control.input-circle.width-150(type='text', name='customer_search', placeholder='search...')
          .tools.col-md-1.right
            a.expand(href='javascript:;', data-original-title='', title='')
      .portlet-body.display-hide
        form#customer-form
          .row
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | First Name
                span.required(aria-required='true') *
              .col-md-12
                .input-group
                  span.input-group-addon
                    i.fa.fa-user
                  .input-icon.right
                    i.fa.fa-info-circle.tooltips(data-original-title='First Name', data-container='body')
                    input.form-control(type='text' name='first_name')

            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label  Middle Name
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='Middle Name', data-container='body')
                  input.form-control(type='text' name='middle_name')

            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | Last Name
                span.required(aria-required='true') *
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='Last Name', data-container='body')
                  input.form-control(type='text' name='last_name')
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | DOB
                span.required(aria-required='true') *
              .col-md-12
                .input-icon.right
                  i.fa.fa-info-circle.tooltips(data-original-title='Date of Birth', data-container='body')
                  input.form-control(type='text' name='date_of_birth')
          .row
            .form-group.col-md-3.col-sm-6.col-xs-12
              label.col-md-12.control-label
                | License Number
                span.required(aria-required='true') *
              .col-md-12
                .input-group
                  span.input-group-addon
                    i.fa.fa-list-alt
                  .input-icon.right
                    i.fa.fa-info-circle.tooltips(data-original-title='Driving License', data-container='body')
                    input.form-control(type='text' name='license_number')

            .form-group.col-md-3.col-sm-6.col-xs-12
