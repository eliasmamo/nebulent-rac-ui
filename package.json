{
  "name": "nebulent-rac-ui",
  "version": "0.0.1",
  "description": "Car rental managment ui client with third party API",
  "main": "app.coffee",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gulp"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "underscore:underscore"
      ]
    },
    "backbone.babysitter": {
      "exports": "Backbone.BabySitter",
      "depends": [
        "backbone:Backbone"
      ]
    },
    "backbone.wreqr": {
      "exports": "Backbone.Wreqr",
      "depends": [
        "backbone:Backbone"
      ]
    },
    "backbone.marionette": {
      "exports": "Marionette",
      "depends": [
        "jquery:jQuery",
        "backbone:Backbone",
        "underscore:_"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone-validation": "^0.11.3",
    "backbone.marionette": "^2.4.1",
    "backbone.radio": "*",
    "backbone.stickit": "^0.9.0",
    "bootstrap-sass": "^3.3.4",
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "coffee-script": "^1.9.2",
    "coffeeify": "^1.1.0",
    "cookie-parser": "^1.3.4",
    "express": "^4.12.3",
    "express-session": "^1.11.1",
    "gulp": "^3.8.11",
    "gulp-coffee": "^2.3.1",
    "gulp-concat": "^2.5.2",
    "gulp-jade": "^1.0.0",
    "gulp-nodemon": "^2.0.2",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.4",
    "jade": "^1.9.2",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "nodemon": "^1.3.7",
    "requirejs": "^2.1.17",
    "underscore": "^1.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasmamo/nebulent-rac-ui.git"
  },
